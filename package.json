  "dependencies": {
    "@zuoque/random-coding": "^0.0.10",
    "canvas": "^2.11.2"
  },
  "devDependencies": {
    "@types/mockjs": "^1.0.10",
    "ts-node": "^10.9.2",
    "typescript": "^5.2.2",
    "vite": "^5.3.1",
    "vite-plugin-dts": "^3.9.1"
  },
  "bugs": {
    "url": "https://github.com/zuoque/id-card/issues"
  },
  "homepage": "https://github.com/zuoque/id-card/tree/main#readme",
  "license": "ISC",
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
